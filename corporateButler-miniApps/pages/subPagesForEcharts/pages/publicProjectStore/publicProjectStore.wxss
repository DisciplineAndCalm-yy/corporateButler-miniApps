/* 新版样式 */
.projectitem_card {
  background-color: #fff;
  border-radius: 20rpx;
  color: #333;
  font-family: Alibaba PuHuiTi 2.0;
  font-size: 28rpx;
  font-style: normal;
  font-weight: 600;
  letter-spacing: -0.3px;
  position: relative;
}
.projectitem_card:not(:first-child) {
  margin-top: 30rpx;
}
.project_titles {
  padding: 30rpx 30rpx 20rpx;
  padding-left: 60rpx;
  display: flex;
  align-items: center;
  position: relative;
}
.project_subtitle {
  position: absolute;
  left: 50rpx;
  top: 50%;
  transform: translate(-100%, -20%);
  font-size: 20rpx;
  color: #0F7CEE;
  margin-right: 6rpx;
}
.project_content {
  padding: 0 30rpx 30rpx 60rpx;
  border-bottom: 1px solid #e9e9e9;
}
.project_item {
  color: #333;
  font-size: 26rpx;
  font-weight: 500;
  margin: 10rpx 0;
}
.project_item_label {
  color: #999;
  font-weight: 400;
}
.project_btnwrap {
  padding: 20rpx 60rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  color: #3F74F6;
  font-family: Alibaba PuHuiTi 2.0;
  font-size: 28rpx;
  font-style: normal;
  font-weight: 400;
}
.more_btnwrap {
  position: absolute;
  right: 0;
  width: 200rpx;
  background-color: #fff;
  border-radius: 0 0 20rpx 20rpx;
  padding: 10rpx;
  color: #3F74F6;
  font-family: Alibaba PuHuiTi 2.0;
  font-size: 28rpx;
  font-style: normal;
  font-weight: 400;
  z-index: 99;
}
.more_btnwrap view {
  padding: 14rpx 20rpx;
}
/* 底部空元素 */
.empty {
  padding-bottom: calc(120rpx + env(safe-area-inset-bottom) / 2);
}
.has_nodata {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  color: rgba(153, 153, 153, 0.60);
  font-family: PingFang SC;
  font-size: 28rpx;
  font-style: normal;
  font-weight: 400;
  line-height: normal;
  letter-spacing: 6rpx;
  padding: 40rpx 0;
}
.nodata_img {
  width: 300rpx;
  height: 300rpx;
}