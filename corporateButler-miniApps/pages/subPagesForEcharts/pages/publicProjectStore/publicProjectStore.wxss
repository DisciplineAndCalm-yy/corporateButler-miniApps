/* pages/subPagesForEcharts/pages/publicProjectStore/publicProjectStore.wxss */
.publicProjectStore {
  background: #f5f6fa;
  overflow: hidden;
}
.store_title_class {
  background: #fff!important;
}
.searchBox {
  padding: 0 40rpx;
  background-color: #fff;
}
.searchBox .t-input {
  padding: 20rpx;
  border-radius: 20rpx;
  color: #999;
  background-color: #f5f6fa;
}
.searchBox .t-input--border {
  position: static;
}
.searchBox .t-input .t-icon-search {
  color: #999;
}
.selectBox {
  display: flex;
  background-color: #fff;
}
.selectBox .t-dropdown-menu__item .t-dropdown-menu__title {
  font-size: 28rpx!important;
}
.selectBox .t-dropdown-menu__item .t-icon {
  font-size: 36rpx!important;
}
.dropActive1 .t-dropdown-menu__item:nth-child(1) {
  border: none!important;
  font-weight: bolder;
  color: #0f7cee!important;
}
.dropActive2 .t-dropdown-menu__item:nth-child(2) {
  border: none!important;
  font-weight: bolder;
  color: #0f7cee!important;
}
.dropActive3 .t-dropdown-menu__item:nth-child(3) {
  border: none!important;
  font-weight: bolder;
  color: #0f7cee!important;
}
.selectBox_item, .drop_active {
  flex: 1;
}
.selectBox_item .t-dropdown-menu__item {
  border: none!important;
  color: #666!important;
}
.drop_active .t-dropdown-menu__item {
  border: none!important;
  color: #0f7cee!important;
}
.searchSelection {
  height: 100rpx;
  background-color: #fff;
  display: flex;
}
/* 内容 */
.publicProjectStore_content {
  height: calc(100% - 150rpx - env(safe-area-inset-bottom) / 2);
  padding: 30rpx;
}
