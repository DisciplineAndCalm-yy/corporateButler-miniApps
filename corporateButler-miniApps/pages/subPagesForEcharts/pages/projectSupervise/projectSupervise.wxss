/* pages/subPagesForEcharts/pages/projectSupervise/projectSupervise.wxss */
.projectSupervise {
  background-color: #f3f5fa;
}
.projectSupervise_title_class {
  background-color: #fff;
}
/* 内容区 */
.projectSupervise_content {
	padding: 40rpx;
}
.projectSupervise_card {
	padding: 20rpx 30rpx 40rpx;
	border-radius: 20rpx;
	background: #FFF;
	color: #333;
	font-family: Alibaba PuHuiTi 2.0;
	font-size: 28rpx;
	font-style: normal;
	font-weight: 400;
	letter-spacing: -0.3px;
}
.projectSupervise_card:not(:first-child) {
	margin-top: 30rpx;
}
.projectSupervise_item {
	padding: 20rpx 0;
	display: flex;
	align-items: center;
	justify-content: space-between;
}
.projectSupervise_item:not(:first-child) {
	border-top: 1px solid #e9e9e9;
}
.projectSupervise_item_label {
	color: #666;
	white-space: nowrap;
}
.projectSupervise_btnwrap {
	margin-top: 40rpx;
	display: flex;
	align-items: center;
	justify-content: flex-end;
}
.projectSupervise_btns {
	width: 200rpx;
	height: 60rpx;
	line-height: 60rpx;
	text-align: center;
	color: #fff;
	border-radius: 10rpx;
	background: #187FFF;
}
/* 底部空元素 */
.empty {
  padding-bottom: calc(env(safe-area-inset-bottom) / 2);
}
.has_nodata {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  color: rgba(153, 153, 153, 0.60);
  font-family: PingFang SC;
  font-size: 28rpx;
  font-style: normal;
  font-weight: 400;
  line-height: normal;
  letter-spacing: 6rpx;
  padding: 40rpx 0;
}
.nodata_img {
  width: 300rpx;
  height: 300rpx;
}