.beg_depart_progress_label {
	margin-bottom: 8rpx;
	color: #333;
	font-family: Alibaba PuHuiTi 2.0;
	font-size: 24rpx;
	font-weight: 400;
}
.beg_depart_score {
	color: #187FFF;
	font-family: Alibaba PuHuiTi 2.0;
	font-size: 28rpx;
	font-style: normal;
	font-weight: 400;
	letter-spacing: -0.3px;
	white-space: nowrap;
