/* 弹窗 */
.detail_wraper {
  width: 80vw;
  padding: 40rpx;
}
.detail_label {
  margin-bottom: 30rpx;
  font-weight: 700;
}
.detail_btn {
  width: 200rpx;
  height: 60rpx;
