/* pages/subPagesForLeader/pages/homeLeader/homeLeader.wxss */
.homeLeader {
	background: #F5F6FA;
}
.leader_top {
	height: 320rpx;
	background: linear-gradient(180deg, #325DFF 0%, #6081FF 70.11%, rgba(95, 126, 244, 0.00) 94.32%);
}
.leader_top_title {
	color: #fff;
	text-align: center;
}
/* 顶部盒子 */
.leader_top_box {
	margin: 40rpx;
	width: calc(100vw - 40)rpx;
	height: 160rpx;
	border-radius: 20rpx;
	background-color: #fff;
	display: flex;
	align-items: center;
}
.leader_top_items {
	width: 33%;
	text-align: center;
	color: #666;
	font-family: Alibaba PuHuiTi 2.0;
	font-size: 26rpx;
	font-style: normal;
	font-weight: 400;
	line-height: normal;
}
.leader_top_items_numbers {
	display: block;
	color: #187FFF;
	text-align: center;
	font-size: 36rpx;
	font-weight: 700;
	margin-bottom: 6rpx;
}
.green_number {
	color: #13CA03;
}
.orange_number {
	color: #FA6521;
}
.leader_top_splitline {
	width: 1px;
	height: 50rpx;
	background: #DDD;
}
/* 内容区 */
.leader_content_wraper {
	padding: 0 40rpx;
	padding-bottom: calc(120rpx + env(safe-area-inset-bottom)/2);
}
.content_topmenu_wraper {
	display: flex;
	align-items: center;
	justify-content: space-between;
}
/* 导航菜单 */
.content_topmenu {
	margin: 0 20rpx;
	color: #187FFF;
	font-family: Alibaba PuHuiTi 2.0;
	font-size: 28rpx;
	font-style: normal;
	font-weight: 400;
	line-height: normal;
	letter-spacing: -0.3px;
	padding-bottom: 14rpx;
	border-bottom: 6rpx solid rgba(255, 255, 255, 0);
	border-radius: 4rpx;
	margin-bottom: 30rpx;
}
.active_menu {
	font-weight: 700;
	border-bottom: 6rpx solid #187FFF;
}
/* 公共标题样式 */
.public_wraper {
	display: flex;
	align-items: center;
	justify-content: space-between;
}
.public_title {
	display: flex;
	align-items: center;
	color: #333;
	font-family: Alibaba PuHuiTi 2.0;
	font-size: 32rpx;
	font-style: normal;
	font-weight: 600;
}
.public_title_icon {
	width: 8rpx;
	height: 45rpx;
	flex-shrink: 0;
	border-radius: 8rpx;
	background: #187FFF;
	margin-right: 20rpx;
}
.public_title_time {
	color: #333;
	font-family: Alibaba PuHuiTi 2.0;
	font-size: 24rpx;
	font-style: normal;
	font-weight: 400;
	line-height: normal;
	letter-spacing: -0.3px;
}
/* 重大项目 */
.public_timewraper {
	margin: 30rpx 0 20rpx;
	padding: 0 20rpx;
	height: 80rpx;
	border-radius: 20rpx;
	background: #FFF;
	display: flex;
	align-items: center;
	justify-content: space-between;
}
.public_timewraper_label {
	display: flex;
	align-items: center;
	color: #333;
	font-family: Alibaba PuHuiTi 2.0;
	font-size: 28rpx;
	font-style: normal;
	font-weight: 400;
}
.timewrap_moreicon {
	font-size: 32rpx;
	color: #187FFF;
	margin-right: 20rpx;
}
.public_timewraper_time {
	color: #187FFF;
	font-family: Alibaba PuHuiTi 2.0;
	font-size: 24rpx;
	font-style: normal;
	font-weight: 400;
	position: relative;
	padding-right: 20rpx;
	display: flex;
	align-items: center;
}
.active_date {
	font-weight: 700;
}
.public_timewraper_time_desc {
	padding: 0 10rpx;
}
.public_timewraper_timeicon {
  content: '';
  width: 14rpx;
  height: 14rpx;
	position: absolute;
	top: 50%;
  right: 0rpx;
  border-left: 3rpx solid #187FFF;
  border-bottom: 3rpx solid #187FFF;
  -webkit-transform: translate(0, -50%) rotate(225deg);
  transform: translate(0, -50%) rotate(225deg);
}
.public_title_subtitle {
	color: #1571F6;
	font-family: Alibaba PuHuiTi 2.0;
	font-size: 28rpx;
	font-style: normal;
	font-weight: 400;
	letter-spacing: -0.3px;
}
/* 重大项目-项目统管 */
.leader_projectManage_wraper {
	margin: 0rpx 0 40rpx;
  white-space: nowrap;
	display: flex;
	align-items: center;
	overflow-x: scroll;
}
.leader_project_item {
  display: inline-block;
	width: 350rpx;
	height: 160rpx;
	flex-shrink: 0;
	border-radius: 10px;
	background-color: #2C80FF;
	/* background: linear-gradient(180deg, #007DFF 0%, #2C80FF 100%); */
}
.leader_project_item:not(:first-child) {
	margin-left: 20rpx;
}
.leader_prjectitem_wrap {
	width: 310rpx;
	height: 120rpx;
	padding: 20rpx;
	display: flex;
	align-items: center;
	justify-content: space-between;
}
.leader_prjectitem_left {
	color: #FFF;
	font-family: Alibaba PuHuiTi 2.0;
	font-size: 24rpx;
	font-style: normal;
	font-weight: 400;
}
.leader_prjectitem_left_number {
	margin: 10rpx 0;
	font-size: 36rpx;
	font-weight: 700;
}
.leader_prjectitem_right {
	width: 56rpx;
	height: 56rpx;
	line-height: 56rpx;
	text-align: center;
	background-color: #fff;
	border-radius: 50%;
}
.ld_prjecticon {
	font-size: 32rpx;
	color: #187FFF;
}
/* 重大项目-省市县重点项目调度中心 */
.leader_province_desc {
	margin: 20rpx 40rpx 0rpx;
	color: #999;
	font-family: Alibaba PuHuiTi 2.0;
	font-size: 24rpx;
	font-style: normal;
	font-weight: 400;
	line-height: normal;
	letter-spacing: -0.3px;
}
.leader_province_wraper {
	margin-top: 30rpx;
	display: flex;
	align-items: center;
	justify-content: space-around;
}
.leader_province_itembox {
	display: flex;
	flex-direction: column;
	align-items: center;
	justify-content: center;
}
.leader_province_item {
	width: 94rpx;
	height: 94rpx;
	line-height: 94rpx;
	text-align: center;
	color: #FFF;
	font-family: Alibaba PuHuiTi 2.0;
	font-size: 48rpx;
	font-style: normal;
	font-weight: 700;
	border-radius: 10px;
	background: linear-gradient(180deg, #007DFF 0%, #1870F5 100%);
}
.province_orangeitem{
	background: linear-gradient(222deg, #FA7340 -4.55%, #F95B0E 98.08%);
}
.province_greenitem{
	background: linear-gradient(225deg, #17B387 -4.55%, #37CFA2 103.37%);
}
.leader_province_label {
	margin: 10rpx 0 30rpx;
	color: #333;
	font-family: Alibaba PuHuiTi 2.0;
	font-size: 24rpx;
	font-style: normal;
	font-weight: 400;
}
/* 重大项目-项目进展统一调度 */
.leader_progress_wraper {
	display: flex;
	flex-wrap: wrap;
}
.leader_progress_itembox {
	min-width: 180rpx;
	height: 110rpx;
	margin: 0 10rpx 20rpx;
	flex: 1;
	padding: 20rpx;
	display: flex;
	align-items: center;
	justify-content: space-between;
	background-color: #007DFF;
	border-radius: 20rpx;
	color: #FFF;
	font-family: Alibaba PuHuiTi 2.0;
	font-size: 24rpx;
	font-style: normal;
	font-weight: 400;
}
.leader_progress_itemleft_fonts {
	margin-bottom: 6rpx;
}
.leader_progress_itemleft_nums {
	margin-right: 10rpx;
	font-size: 36rpx;
	font-weight: 700;
}
.leader_progress_itemright {
	width: 108rpx;
	height: 108rpx;
	border-radius: 50%;
	display: flex;
	flex-direction: column;
	align-items: center;
	justify-content: center;
	background-color: #0F77E3;
}
.progress_deepcolor_orange {
	background-color: #F05511;
}
.progress_deepcolor_green {
	background-color: #1AB387;
}
.progress_deepcolor_yellow {
	background-color: #DAC618;
}
.leader_progressicon {
	font-size: 36rpx;
	margin-bottom: 4rpx;
}
/* 重大项目-项目督办 */
.leader_project_supervise {
	margin-top: 20rpx;
	background-color: #fff;
	border-radius: 20rpx;
}
.leader_project_supervise_top {
	padding: 30rpx 30rpx 0;
}
.project_supervise_label {
	color: #333;
	font-family: Alibaba PuHuiTi 2.0;
	font-size: 28rpx;
	font-style: normal;
	font-weight: 700;
	line-height: normal;
	margin-bottom: 20rpx;
}
.project_supervise_tags {
	display: flex;
	flex-wrap: wrap;
	align-items: center;
	justify-content: flex-start;
	color: #1571F6;
	font-family: Alibaba PuHuiTi 2.0;
	font-size: 24rpx;
	font-style: normal;
	font-weight: 400;
	letter-spacing: -0.3px;
}
.project_supervise_tagitem {
	padding: 6rpx 12rpx;
	border-radius: 6rpx;
	opacity: 0.8;
	background: #D5E1FF;
