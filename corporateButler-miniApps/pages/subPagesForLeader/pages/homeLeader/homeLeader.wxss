}
.project_supervise_tagitem:not(:last-child) {
	margin-right: 20rpx;
}
.project_supervise_progress {
	padding: 0 30rpx;
	display: flex;
	align-items: center;
	justify-content: space-between;
	margin-bottom: 30rpx;
	color: #333;
	font-family: Alibaba PuHuiTi 2.0;
	font-size: 24rpx;
	font-style: normal;
	font-weight: 400;
}
.project_supervise_progresswraper {
	display: flex;
	align-items: center;
}
.project_supervise_progress_icon {
	margin-right: 14rpx;
	width: 16rpx;
	height: 16rpx;
	border-radius: 50%;
	background-color: #E2D247;
}
.supervise_iconcolor_orange {
	background-color: #FA6625;
}
.supervise_iconcolor_red {
	background-color: #d6001b;
}
.leader_project_supervise_bottom {
	padding: 20rpx;
	display: flex;
	align-content: center;
	justify-content: space-around;
	color: #1770F6;
	font-family: Alibaba PuHuiTi 2.0;
	font-size: 28rpx;
	font-style: normal;
	font-weight: 400;
	border-top: 1px solid #E9E9E9;
}
.project_supervise_splitline {
	width: 1px;
	height: 22rpx;
	background-color: #E9E9E9;
}
.leader_project_supervise_more {
	margin-top: 30rpx;
	text-align: center;
	color: #666;
	font-size: 24rpx;
}
/* 惠企政策 */
/* 惠企政策-政策统计 */
.policy_totals {
	margin: 30rpx 0;
	height: 160rpx;
	display: flex;
	align-items: center;
	justify-content: space-around;
}
.policy_totals_item {
	flex: 1;
	position: relative;
}
.policy_totals_imgs {
	width: 220rpx;
	height: 160rpx;
}
.policy_totals_box {
	position: absolute;
	top: 0;
	left: 0;
	right: 0;
	bottom: 0;
	text-align: center;
	color: #FFF;
	font-family: Alibaba PuHuiTi 2.0;
	font-size: 24rpx;
	font-style: normal;
	font-weight: 400;
}
.policy_totals_numbers {
	margin-top: 20rpx;
	font-size: 40rpx;
	font-weight: 700;
	margin-bottom: 16rpx;
}
/* 惠企政策-政策订阅 */
.policy_setting {
	border-radius: 10rpx;
	background: linear-gradient(180deg, #FFF 0%, #FFF 21.77%);
}
.policy_tagwrap {
	padding: 30rpx;
	display: flex;
	flex-wrap: wrap;
	align-items: center;
	justify-content: flex-start;
}
.policy_tag {
	margin: 10rpx;
	padding: 10rpx;
	width: 110rpx;
	height: 70rpx;
	display: flex;
	align-items: center;
	justify-content: center;
	border-radius: 8rpx;
	border: 2px solid #187FFF;
	background: #FFF;
	color: #333;
	font-family: Alibaba PuHuiTi 2.0;
	font-size: 24rpx;
	font-style: normal;
	font-weight: 400;
	letter-spacing: -0.3px;
	position: relative;
}
.policy_tag_hot {
	width: 56rpx;
	height: 56rpx;
	position: absolute;
	top: 0;
	right: 0;
}
.policy_tolist {
	padding: 20rpx 0;
	border-top: 1px solid #E9E9E9;
	color: #187FFF;
	text-align: center;
	font-family: Alibaba PuHuiTi 2.0;
	font-size: 28rpx;
	font-style: normal;
	font-weight: 400;
}
/* 企业诉求 */
.beg_totals {
	margin-top: 0;
}
/* 企业诉求-事项处理统计 */
.beg_things {
	margin: 30rpx 0;
}
.beg_things_menus {
	display: flex;
	align-items: center;
}
.beg_things_menuitem {
	width: calc(50vw - 50rpx);
	text-align: center;
	height: 80rpx;
	line-height: 80rpx;
	color: #666;
	text-align: center;
	font-family: Alibaba PuHuiTi 2.0;
	font-size: 28rpx;
	font-style: normal;
	font-weight: 400;
	letter-spacing: -0.3px;
}
.active_begmenu1 {
	color: #187FFF;
	font-weight: 700;
	background-color: #fff;
	border-radius: 20rpx 10rpx 0 0;
}
.active_begmenu2 {
	color: #187FFF;
	font-weight: 700;
	background-color: #fff;
	border-radius: 0 20rpx 0 10rpx;
}
.beg_left {
	width: 0;
	height: 0;
	border-bottom: 80rpx solid #fff;
	border-right: 50rpx solid #F5F6FA;
	border-radius: 10rpx 0 0 0;
}
.beg_right {
	width: 0;
	height: 0;
	border-bottom: 80rpx solid #fff;
	border-left: 50rpx solid #F5F6FA;
	border-radius: 0 0 10rpx 0;
}
.beg_things_wrap {
	height: 220rpx;
	background-color: #fff;
	border-radius: 0 0 20rpx 20rpx;
}
.beg_things_progressitem {
	display: flex;
	align-items: center;
	justify-content: space-between;
	padding: 20rpx 30rpx;
	color: #333;
	font-family: Alibaba PuHuiTi 2.0;
	font-size: 24rpx;
	font-weight: 400;
}
.beg_things_progress {
	width: 70%;
}
.beg_things_progress_numbers {
	color: #187FFF;
}
/* 企业诉求-服务管家统计 */
.beg_steward {
	margin: 30rpx 0;
}
.beg_steward_item {
	padding: 20rpx 20rpx 20rpx 0;
	display: flex;
	align-items: center;
	justify-content: space-between;
	border-radius: 10rpx;
	background: linear-gradient(180deg, #FFF 0%, #FFF 21.77%);
}
.beg_steward_item:first-child {
	margin-bottom: 20rpx;
}
.beg_steward_iconwrap {
	width: 70rpx;
	height: 70rpx;
	line-height: 70rpx;
	text-align: center;
	border-radius: 0px 10px 10px 0px;
	background: #187FFF;
}
.beg_steward_icon {
	font-size: 42rpx;
	color: #fff;
}
.beg_steward_itembox {
	width: 25%;
	text-align: center;
	color: #333;
	font-family: Alibaba PuHuiTi 2.0;
	font-size: 24rpx;
	font-style: normal;
	font-weight: 400;
}
.beg_steward_itembox_number {
	color: #187FFF;
	font-size: 36rpx;
	font-weight: 700;
}
.steward_green_bg {
	background-color: #2FC096;
}
.steward_green {
	color: #2FC096;
}
/* 企业诉求-事项投诉类型排行 */
.complain_wrap {
	height: 200rpx;
	padding: 0 40rpx;
}
.complain_wrap_overflow {
	height: 100%;
	display: flex;
	align-items: center;
	overflow-x: scroll;
}
.complain_item {
	padding: 20rpx;
	width: 120rpx;
	color: #333;
	text-align: center;
	font-family: Alibaba PuHuiTi 2.0;
	font-size: 24rpx;
	font-style: normal;
	font-weight: 400;
	letter-spacing: -0.3px;
}
.complain_rankwrap {
	margin: 0 auto 14rpx;
	width: 80rpx;
	height: 80rpx;
	background-color: #187FFF;
	border-radius: 50%;
	display: flex;
	align-items: center;
	justify-content: center;
}
.complain_ranks {
	width: 70rpx;
	height: 70rpx;
	line-height: 70rpx;
	text-align: center;
	background-color: #fff;
	border-radius: 50%;
}
.complain_ranks_label {
	width: 100%;
	overflow: hidden;
	white-space: nowrap;
	text-overflow: ellipsis;
}
.complain_ranks_numbers {
	color: #187FFF;
	font-family: Alibaba PuHuiTi 2.0;
	font-size: 32rpx;
	font-style: normal;
	font-weight: 700;
	letter-spacing: -0.3px;
}
.complain_orange_bg {
	background-color: #FA6625;
}
.complain_orange {
	color: #FA6625;
}
.complain_green_bg {
	background-color: #2FC096;
}
.complain_green {
	color: #2FC096;
}
.complain_yellow_bg {
	background-color: #E2D247;
}
.complain_yellow {
	color: #E2D247;
}
/* 企业诉求-各部门评价排名 */
.beg_depart {
	margin: 30rpx 0;
}
.beg_depart_item {
	padding-right: 20rpx;
	display: flex;
	align-items: center;
	height: 100rpx;
	border-radius: 10rpx;
	background: linear-gradient(180deg, #FFF 0%, #FFF 21.77%);
}
.beg_depart_item:not(:first-child) {
	margin-top: 10rpx;
}
.beg_depart_ranks {
	width: 20%;
	height: 100%;
	display: flex;
	align-items: center;
	justify-content: center;
}
.depart_ranks_nocup {
	width: 40rpx;
	height: 40rpx;
	line-height: 40rpx;
	text-align: center;
	background-color: #0C77FB;
	border-radius: 50%;
	color: #FFF;
	font-size: 24rpx;
	font-weight: 700;
}
.depart_ranks_cup {
	position: relative;
}
.rank_first {
	font-size: 50rpx;
	color: #FFED48;
}
.rank_second {
	font-size: 50rpx;
	color: #DADADA;
}
.rank_third {
	font-size: 50rpx;
	color: #DFA348;
}
.rank_numbers {
	position: absolute;
