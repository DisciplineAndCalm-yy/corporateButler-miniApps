/* pages/mine/mine.wxss */
.mine {
  background: #fff;
}
/* 无企业信息 */
.mine_wrap {
  height: 100%;
  background-color: #f5f6fa;
}
.no_entinfo {
  padding: 40rpx 0 0;
  text-align: center;
}
.no_entinfo_topbox {
  position: relative;
}
.no_entinfo .no_entinfo_imgs {
  width: 435rpx;
  height: 370rpx;
}
.no_entinfo_boxs {
  margin: 0 auto;
  position: absolute;
  top: 45%;
  left: 0;
  right: 0;
  width: 360rpx;
  height: 100rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  background: #6B96FF;
  border-radius: 20rpx;
  font-weight: 700;
  font-size: 28rpx;
  color: #fff;
}
.no_entinfo_boxs_iconbox {
  display: inline-block;
  width: 60rpx;
  height: 60rpx;
  line-height: 60rpx;
  background-color: #fff;
  border-radius: 50%;
  margin-right: 10rpx;
}
.blue_icon {
  font-size: 36rpx;
  color: #3F74F6;
}
.no_entinfo_tips {
  margin: 20rpx 0 40rpx;
  color: #333;
  font-weight: 700;
  font-size: 46rpx;
  letter-spacing: 1px;
}
.no_entinfo_desc {
  color: #999;
  font-weight: 400;
  font-size: 28rpx;
  letter-spacing: -0.3px;
}
.no_entinfo_btns {
  margin-top: 100rpx;
  width: 80%;
  height: 90rpx;
  line-height: 90rpx;
  font-weight: 700;
  font-size: 32rpx;
  letter-spacing: 1px;
  color: #fff;
  background-color: #3F74F6;
  border-radius: 46rpx;
}
.login_out {
  background-color: #FF4E4E;
}
/* 有企业信息 */
.has_entinfo {
  position: relative;
  background-color: #f5f6fa;
}
.has_top_wrap {
  position: absolute;
  top: 0;
  width: 100%;
  height: 540rpx;
  z-index: 1;
}
.has_entinfo_wrap {
  position: relative;
  z-index: 2;
  padding: 40rpx;
}
.has_entinfo_moreent {
  font-weight: 400;
  font-size: 26rpx;
  text-align: right;
  letter-spacing: -0.3px;
  color: #333333;
}
.has_entinfo_moreent_cards{
  display: inline-block;
}
.has_entinfo_moreent_icon {
  position: relative;
  display: inline-block;
  width: 30rpx;
  height: 30rpx;
}
.has_entinfo_moreent_icon::after{
  content: '';
  width: 16rpx;
  height: 16rpx;
  position: absolute;
  right: 0;
  bottom: 0;
  border-left: 4rpx solid #000;
  border-bottom: 4rpx solid #000;
  -webkit-transform: translate(0, 0) rotate(-135deg);
  transform: translate(0, 0) rotate(-135deg);
}
.has_entinfo_entname {
  margin: 30rpx 0 20rpx;
  font-weight: 700;
  font-size: 40rpx;
  letter-spacing: -0.3px;
  color: #020202;
}

.has_entinfo_entcodes {
  display: flex;
  align-items: center;
  justify-content: space-between;
}
/* 企业信用code */
.has_entinfo_entcode_wrap {
  position: relative;
  margin-top: 10rpx;
  background: #FFF5E6;
  border: 1px solid #EAB445;
  padding: 4rpx 12rpx 4rpx 50rpx;
  border-radius: 40rpx;
  height: 30rpx;
  line-height: 30rpx;
  color: #EAB445;
  font-weight: 400;
  font-size: 24rpx;
}
.entcode_icon_wrap {
  position: absolute;
  top: 0;
  left: 0;
  display: inline-block;
  width: 40rpx;
  height: 40rpx;
  line-height: 40rpx;
  font-weight: 500;
  font-size: 28rpx;
  color: #fff;
  background: linear-gradient(212.91deg, #FFE08D 15.8%, #FF9900 91.13%);
  transform: rotate(45deg);
}
.entcode_icon_wrap_taxicon {
  display: inline-block;
  width: 40rpx;
  height: 40rpx;
  line-height: 40rpx;
  text-align: center;
  background: linear-gradient(212.91deg, #FFE08D 15.8%, #FF9900 91.13%);
  transform: rotate(-45deg);
}
/* 标签tag */
.has_entinfo_enttags {
  margin-right: -40rpx;
  padding: 10rpx 40rpx 10rpx 20rpx;
  font-weight: 400;
  font-size: 26rpx;
  text-align: center;
  letter-spacing: -0.3px;
  color: #FFFFFF;
  background: #3F74F6;
  border-radius: 40rpx 0px 0px 40rpx;
}
/* 管家黑金卡 */
.has_entinfo_steward {
  position: relative;
  margin-top: 30rpx;
  height: 90rpx;
}
.has_entinfo_steward_imgs {
  position: absolute;
  top: 0;
  width: 100%;
  height: 90rpx;
  z-index: 1;
}
.has_entinfo_steward_wrap {
  position: relative;
  z-index: 2;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: space-around;
  font-weight: 400;
  font-size: 28rpx;
  text-align: center;
  letter-spacing: -0.3px;
  color: #FFE298;
}
.has_entinfo_steward_icons_wrap {
  display: flex;
  align-items: center;
}
.has_entinfo_steward_icons {
  width: 32rpx;
  height: 30rpx;
  margin-right: 8rpx;
}
.has_entinfo_steward_btns {
  width: 140rpx;
  height: 50rpx;
  line-height: 50rpx;
  margin: 0;
  padding: 0 10rpx;
  background: linear-gradient(90deg, #FFE297 -21.63%, #FFBF18 137.14%);
  border-radius: 10rpx;
  font-weight: 400;
  font-size: 28rpx;
  text-align: center;
  letter-spacing: -0.3px;
  color: #542D00;
}
/* 管家卡 */
.has_entinfo_steward_cardwrap {
  margin-top: 50rpx;
  position: relative;
}
.steward_bgcards {
  position: absolute;
  top: 0;
  z-index: 1;
  width: 100%;
  height: 100rpx;
  background: rgba(190, 208, 255, 0.7);
  border-radius: 40rpx;
  transform: rotate(4.81deg);
}
.steward_cardswrap {
  position: relative;
  z-index: 2;
}
.steward_cards {
  height: 150rpx;
  display: flex;
  margin-bottom: 20rpx;
  padding-bottom: 30rpx;
  border-bottom: 1px solid #F2F2F2;
  padding: 30rpx 40rpx;
  background-color: #fff;
  border-radius: 20rpx;
}
/* .steward_cards:last-child {
  margin: 0;
  padding: 0;
  border: none;
} */
.newCards .steward_cards_left {
  margin-left: 60rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-evenly;
}
.newCards .steward_phonenum {
  margin-left: 0;
  font-size: 30rpx;
}
.newCards .steward_cards_phonenum {
  font-size: 30rpx;
}
.steward_cards_infos {
  font-weight: 700;
  font-size: 30rpx;
  letter-spacing: -0.3px;
  color: #000;
}
.steward_phonenum {
  margin-left: 30rpx;
  margin-right: 10rpx;
  color: #3F74F6;
  font-size: 24rpx;
}
.steward_cards_phonenum {
  color: #3F74F6;
  font-weight: 400;
  font-size: 24rpx;
}
.steward_cards_desc {
  margin: 12rpx 0 10rpx;
  font-weight: 400;
  font-size: 24rpx;
  letter-spacing: -0.3px;
  color: #666;
}

.steward_cards_labellist {
  display: flex;
  align-items: center;
}
.steward_cards_labelitem {
  margin-right: 10rpx;
  padding: 8rpx 20rpx;
  font-size: 24rpx;
  font-weight: 400;
  text-align: center;
  letter-spacing: -0.3px;
  color: #A6AFCD;
  border-radius: 54rpx;
  background-color: rgba(246, 249, 254, 1);
}
.steward_cards_right {
  margin-left: 40rpx;
  width: 120rpx;
}
.steward_cards_iconwraps {
  margin: 0 auto;
  width: 80rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  background: #FFF;
  border: 4rpx solid rgba(28, 28, 28, 0.2);
  border-radius: 50%;
}
.steward_cards_avatarfont {
  font-size: 36rpx;
  color: #999;
}
.steward_avatar_icons {
  font-size: 60rpx;
  color: #d2d2d2;
}
.steward_cards_level {
  width: 100rpx;
  margin-top: 20rpx;
  padding: 4rpx 10rpx 6rpx;
  color: #fff;
  text-align: center;
  font-weight: 400;
  font-size: 24rpx;
  background: #3F74F6;
  border-radius: 30rpx;
  display: flex;
  align-items: center;
  justify-content: space-evenly;
}
.level_icon {
  font-size: 24rpx;
}
/* 我的-导航卡 */
.mine_info_cards {
  margin-top: 40rpx;
  padding: 20rpx 0rpx 40rpx 40rpx;
  background-color: #fff;
  border-radius: 20rpx;
  font-weight: 400;
  font-size: 28rpx;
  color: #333333;
}
.mine_info_cards_item {
  margin-top: 10rpx;
  display: flex;
  align-items: center;
}
.mine_info_cards_icon {
  display: inline-block;
  width: 34rpx;
  height: 32rpx;
}
.mine_info_cards_item .icons {
  width: 36rpx;
  height: 32rpx;
}
.mine_info_card_label {
  width: 100%;
  padding: 20rpx;
  border-bottom: 1px solid #F2F2F2;
}
.mine_info_cards_item:last-child .mine_info_card_label {
  border: none;
}



.userinfo {
  display: flex;
  padding: 40px;
  align-items: center;
}
.userinfo-avatar {
  width: 80px;
  height: 80px;
  margin-right: 30px;
}
.imageBox {
  margin: 3%;
  background: white;
  border-radius: 30rpx;
  padding: 30rpx;
  font-size: 28rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.imageBox image {
  width: 100rpx;
  height: 100rpx;
}

.changeent_wrap {
  height: 20vh;
  padding: 40rpx 0rpx 40rpx;
}
.changeent_wrap .t-radio-group {
  height: 100%;
  overflow-y: scroll;
}
.card {
  position: relative;
  margin: 32rpx;
  border-radius: 12rpx;
  overflow: hidden;
  box-sizing: border-box;
  border: 3rpx solid #e7e7e7;
}

.card--active {
  border-color: #0052d9;
}

.card--active::after {
  content: '';
  display: block;
  position: absolute;
  left: 0;
  top: 0;
  width: 0;
  border: 14px solid #0052d9;
  border-bottom-color: transparent;
  border-right-color: transparent;
}

.card__icon {
  color: #fff;
  position: absolute;
  left: 1.5px;
  top: 1.5px;
  z-index: 1;
}