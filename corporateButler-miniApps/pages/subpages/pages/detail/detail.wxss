.container {
    width: 100%;
    background-color: #F5F6FA;
    padding-top: 24rpx;
    padding-bottom: 24rpx;
}
.message_title_class {
    background-color: #fff;
}
.basic {
    width: 690rpx;
    margin: 0rpx auto;
    padding: 0rpx 32px;
    margin-bottom: 10rpx;
    border-radius: 20rpx;
    box-sizing: border-box;
    background-color: #fff;
    position: relative;
    padding-bottom: 36rpx;
    box-shadow: 0px 4px 4px rgba(0, 0, 0, 0.02);
}
.basic::after {
    content: '';
    position: absolute;
    width: 100%;
    height: 1rpx;
    background-color: #EBEBEB;
    top: 84rpx;
    left: 0;
}
.basic-top {
    padding: 26rpx 0 15rpx 0;
    color: #333;
    font-weight: bold;
    font-size: 30rpx;
    position: relative;
    margin-bottom: 46rpx;
}
.basic-top .iconfont {
    width: 14rpx;
    height: 14rpx;
    color: #0F7CEE;
    position: absolute;
    top: 50%;
    margin-top: -10rpx;
    left: -40rpx;
    margin-right: 13rpx;
}
.basic-bottom {
    display: flex;
    font-size: 28rpx;
    margin-bottom: 51rpx;
    justify-content: space-between;
}
.basic-bottom:nth-last-child(1) {
    margin-bottom: 0;
}
.basic-left {
    min-width: 150rpx;
    color: #999;
}
.basic-right {
    color: #333;
    word-break: break-all;
}
.basic-inner-top {
    display: flex;
    justify-content: flex-end;
}
.basic-inner-bottom {
    margin-top: 40rpx;
    color: #666;
}
.basic-service {
    display: flex;
    font-size: 28rpx;
}
.result-title {
    font-size: 28rpx;
    color: #fff;
    display: inline-block;
    padding: 12rpx 23rpx;
    background: #0F7CEE;
    border-radius: 10rpx;
    margin-bottom: 30rpx;
}
.result-content {
    display: flex;
    font-size: 28rpx;
    margin-bottom: 30rpx;
}
.result-content .result{
    width: 300rpx;
    color: #999;
}
.result-content .content {
    color: #333;
}
.result-annex {
    display: flex;
    color: #999;
    font-size: 28rpx;
    justify-content: space-between;
}
.basic-result {
    margin-bottom: 25rpx;
}
.annex-content {
    width: 450rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    color: #0F7CEE;
}
.basic-button {
    display: flex;
}
.basic-button button {
    width: 122rpx;
    height: 56rpx;
    border-radius: 6rpx;
    color: #fff;
    text-align: center;
    font-weight: 400;
    font-size: 28rpx;
    margin-top: 28rpx;
    line-height: 56rpx;
    background-color: #0F7CEE;
}
.basic-button .back {
    color: #999;
    background-color: #fff;
}
.t-dialog .t-class {
    width: 500rpx;
}
.basic-text {
    font-size: 28rpx;
    color: #333;
}
.basic-text.other {
    margin-left: 22rpx;
}
.speed-span {
    text-align: left;
    font-size: 24rpx;
    word-break: break-all;
}
.span-color {
    color: #999;
}
.result-content .result {
    width: 150rpx;
}