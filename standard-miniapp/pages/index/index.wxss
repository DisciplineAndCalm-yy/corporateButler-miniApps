.container {
  position: relative;
  width: 100%;
  height: 100vh;
  text-align: center;
  background-color: #2B65D9;
}
.background_img {
  position: absolute;
  width: 100%;
  height: 100%;
  left: 0;
  bottom: 0;
  z-index: 1;
}
.index_content {
  position: relative;
  z-index: 2;
  padding-top: 35%;
}
.logos_wrap {
  margin: 0 auto;
  display: flex;
  align-items: center;
  justify-content: center;
  width: 120rpx;
  height: 120rpx;
  background-color: #fff;
  border-radius: 20rpx;
}
.logos {
  width: 64rpx;
  height: 60rpx;
}
.title {
  margin: 30rpx 0;
  color: #fff;
  font-weight: 600;
  font-size: 36rpx;
  text-align: center;
}
.title_desc {
  font-weight: 400;
  font-size: 22rpx;
  text-align: center;
  letter-spacing: 0.92em;
  color: #999;
}
.btnbox {
  margin-top: 20%;
  text-align: center;
}
.btns {
  width: 80%;
  height: 90rpx;
  line-height: 90rpx;
  background: #fff;
  font-weight: 700;
  font-size: 28rpx;
  color: #2B65D9;
  margin: 50rpx auto 30rpx;
  border-radius: 20rpx;
}
.desc {
  color: #fff;
  font-weight: 400;
  font-size: 24rpx;
}
.agreement {
  display: flex;
  align-items: center;
  justify-content: center;
  margin-top: 10vh;
  color: #fff;
  font-weight: 400;
  font-size: 24rpx;
}
.agreement_topage {
  text-decoration: underline;
}

.agreement checkbox {
  display: flex;
  align-items: center;
}

/* 未选中的背景样式 */
checkbox .wx-checkbox-input{
  width: 25rpx; 
  height: 25rpx; 
  border-radius: 50%;
}

/* 选中后的背景样式 */
checkbox .wx-checkbox-input.wx-checkbox-input-checked{
  border: none;
  background: #37C674;
}

/* 选中后的勾子样式 */
checkbox .wx-checkbox-input.wx-checkbox-input-checked::before{
  width: 25rpx;
  height: 25rpx;
  line-height: 25rpx;
  border-radius: 50%;
  text-align: center;
  font-size: 24rpx; 
  color:#FFF; 
  /* background: transparent;
  transform: translate(-50%, -50%) scale(1);
  -webkit-transform: translate(-50%, -50%) scale(1); */
}