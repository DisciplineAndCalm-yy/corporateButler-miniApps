/* pages/subPagesForEcharts/pages/projectTrack/projectTrack.wxss */
.projectTrack {
  background-color: #f3f5fa;
  overflow-y: scroll;
  margin-bottom: calc(120rpx + env(safe-area-inset-bottom)/2);
}
.projectTrack_title_class {
  background-color: #fff;
}
.contentwrap {
  padding: 40rpx;
  margin-bottom: calc(150rpx + env(safe-area-inset-bottom)/2);
}
/* 项目信息 */
.project_info {
  padding: 40rpx 30rpx;
  background-color: #fff;
  border-radius: 20rpx;
  font-size: 26rpx;
  font-family: Alibaba PuHuiTi 2.0;
  font-style: normal;
  letter-spacing: -0.3px;
  font-weight: 400;
  color: #666;
}
.project_info_title {
  font-size: 28rpx;
  font-weight: bolder;
  color: #333;
  margin-bottom: 30rpx;
}
.project_info_content {
  margin-top: 20rpx;
}
.project_info_content_font {
  color: #333;
  font-weight: bolder;
}
.project_info_tags {
  display: flex;
}
.info_tag {
  margin: 10rpx 0;
  font-weight: 400;
  font-size: 24rpx;
  letter-spacing: -0.3px;
  color: #3F74F6;
  background: #E3EBFF;
  border-radius: 30rpx;
  padding: 6rpx 20rpx;
}
.info_tag:not(:first-child) {
  margin-left: 20rpx;
}
.project_info_toproject {
  margin-top: 30rpx;
  padding: 20rpx 0;
  display: flex;
  justify-content: flex-end;
}
.project_info_toproject_btn {
  padding: 16rpx 40rpx;
  color: #fff;
  border-radius: 100rpx;
  background: #3F74F6;
  font-family: Alibaba PuHuiTi 2.0;
  font-size: 26rpx;
  font-style: normal;
  font-weight: 600;
  letter-spacing: -0.3px;
}
/* tabs标签 */
.track_tabs {
  margin-top: 30rpx;
  display: flex;
  justify-content: center;
  font-weight: 400;
  font-size: 32rpx;
  letter-spacing: -0.3px;
  color: #666;
  margin-bottom: 40rpx;
}
.right_tabs {
  margin-left: 80rpx;
}
/* 激活标题 */
.activetab {
  font-weight: 600;
  color: #333;
  padding-bottom: 10rpx;
  border-bottom: 10rpx solid #3F74F6;
  border-radius: 10rpx;
}
.track_infos {
  /* padding: 20rpx; */
  /* background-color: #fff; */
  /* border-radius: 20rpx; */
}
.track_procedures {
  /* padding: 20rpx; */
  /* background-color: #fff; */
  /* border-radius: 20rpx; */
}
/* 完成投资 */
.info_wraper {
  background-color: #fff;
  /* background-color: #FBFCFF; */
  border-radius: 20rpx;
  padding: 30rpx;
  font-weight: 400;
  font-size: 26rpx;
  letter-spacing: -0.3px;
  color: #999;
}
.info_wraper_title {
  color: #333;
  font-size: 28rpx;
  font-weight: bolder;
  margin-bottom: 40rpx;
}
.info_wraper_font {
  color: #333;
  font-family: Alibaba PuHuiTi 2.0;
  font-style: normal;
  font-weight: 700;
  letter-spacing: -0.3px;
}
.info_wraper_item {
  margin-bottom: 20rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.info_wraper_item_subtitle {
  word-break: keep-all;
  color: #666;
}
.info_wraper_splitline {
  margin: 40rpx 0;
  height: 2rpx;
  background-color: #E9E9E9;
}
/* 季度 */
.complete_wraper {
  /* margin-top: 30rpx; */
  /* padding: 20rpx; */
  /* background-color: #FBFCFF; */
  /* border-radius: 20rpx; */
  font-weight: 400;
  font-size: 28rpx;
  letter-spacing: -0.3px;
  color: #999;
}
.quarter_title {
  font-weight: 600;
  color: #333;
}
.quarter_title_label {
  font-size: 28rpx;
}
.first_quarter {
  position: relative;
  float: right;
  font-weight: 400;
  color: #3F74F6;
  padding-right: 30rpx;
}
.quarter_icons {
  content: '';
  width: 14rpx;
  height: 14rpx;
  position: absolute;
  top: 12rpx;
  right: 0rpx;
  border-left: 4rpx solid #3F74F6;
  border-bottom: 4rpx solid #3F74F6;
  -webkit-transform: translate(0, 0) rotate(225deg);
  transform: translate(0, 0) rotate(225deg);
}
.expend_icon {
  top: 10rpx;
  -webkit-transform: translate(0, 0) rotate(315deg);
  transform: translate(0, 0) rotate(315deg);
}
.quarter_wraper {
  margin-top: 30rpx;
  padding: 30rpx;
  background-color: #fff;
  border-radius: 20rpx;
}
.quarter_wraper:not(:first-child) {
  margin-top: 30rpx;
  padding-top: 30rpx;
  /* border-top: 1px solid #E9E9E9; */
}
/* 月度卡片 */
.complete_month_cards {
  margin-top: 30rpx;
  padding-top: 20rpx;
  border-top: 1px solid #E9E9E9;
}
.month_cards_title {
  font-size: 28rpx;
  letter-spacing: -0.3px;
  color: #3F74F6;
  display: flex;
  align-items: center;
}
.month_cards_icon {
  width: 16rpx;
  height: 16rpx;
  background-color: #3F74F6;
  border-radius: 50%;
  margin-right: 16rpx;
}
.month_cards_item {
  margin: 15rpx 0;
  display: flex;
  align-items: center;
  justify-content: space-between;
  color: #666;
}
.month_cards_item_label {
  display: inline-block;
  word-break: keep-all;
}
.month_cards_input {
  text-align: right;
  color: #333;
  font-weight: bolder;
}
.month_cards_input_active {
  display: inline-block;
  max-width: 200rpx;
  min-width: 100rpx;
  padding: 6rpx 14rpx;
  border: 1px solid #C9C9C9;
  border-radius: 10rpx;
}
.month_cards_textarea {
  width: calc(100% - 30rpx);
  color: #333;
  height: 130rpx;
  font-size: 28rpx;
  font-family: Alibaba PuHuiTi 2.0;
  font-style: normal;
  /* font-weight: 700; */
  padding: 12rpx 20rpx;
  border: 1px solid #C9C9C9;
  border-radius: 20rpx;
}
.month_cards_textarea_bg {
  color: #666;
  background-color: #F3F3F3;
  border: 1px solid #F3F3F3;
}
.month_cards_pickerfont {
  color: #333;
}
/* 流程跟进 */
.procedures_cards {
  margin-top: 30rpx;
  padding: 30rpx;
  background-color: #fff;
  border-radius: 20rpx;
  margin-bottom: 30rpx;
  font-weight: 400;
  font-size: 26rpx;
  letter-spacing: -0.3px;
  color: #333;
}
.mustfill_icon {
  position: absolute;
  left: -16rpx;
  color: #FF0000;
}
.procedures_names {
  font-size: 28rpx;
  display: inline-block;
  max-width: 64%;
}
.procedures_addmore {
  float: right;
  padding-right: 50rpx;
  font-weight: 400;
  color: #3F74F6;
}
.procedures_delete {
  float: right;
  padding-right: 50rpx;
  font-weight: 400;
  color: red;
}
.procedures_cards_label {
  font-weight: 400;
  color: #666;
  font-family: Alibaba PuHuiTi 2.0;
  font-style: normal;
}
.procedures_cards_title {
  position: relative;
  font-weight: 700;
}
.procedures_cards_title_icon {
  content: '';
  width: 14rpx;
  height: 14rpx;
  position: absolute;
  top: 16rpx;
  right: 0rpx;
  border-left: 4rpx solid #3F74F6;
  border-bottom: 4rpx solid #3F74F6;
  -webkit-transform: translate(0, 0) rotate(135deg);
  transform: translate(0, 0) rotate(135deg);
}
.procedures_cards_date {
  margin-top: 40rpx;
  margin-bottom: 20rpx;
  position: relative;
}
.procedures_cards_time_picker {
  float: right;
  color: #333;
  font-family: Alibaba PuHuiTi 2.0;
  font-style: normal;
  font-weight: 700;
}
.procedures_cards_inputs {
  width: 65%;
  float: right;
  text-align: right;
  color: #333;
  font-family: Alibaba PuHuiTi 2.0;
  font-style: normal;
  font-weight: 700;
}
.procedures_cards_nums {
  margin-bottom: 20rpx;
}
.procedures_cards_textarea {
  margin-top: 20rpx;
  width: calc(100% - 30rpx);
  height: 130rpx;
  padding: 12rpx 20rpx;
  font-size: 28rpx;
  font-family: Alibaba PuHuiTi 2.0;
  font-style: normal;
  /* font-weight: 700; */
  border: 1px solid #C9C9C9;
  border-radius: 20rpx;
}
.procedures_cards_textarea_bg {
  color: #666;
  background-color: #F3F3F3;
  border: 1px solid #F3F3F3;
}
.procedures_cards_desc {
  margin-bottom: 20rpx;
  padding-bottom: 20rpx;
}
.procedures_cards_images {
  /* margin-bottom: 20rpx; */
  padding-bottom: 20rpx;
  /* border-bottom: 1px solid #E9E9E9; */
}
.procedures_cards_upload {
  margin-top: 30rpx;
  display: flex;
  flex-wrap: wrap;
}
.image_frame {
  position: relative;
  width: 180rpx;
  height: 180rpx;
  border-radius: 20rpx;
  margin-right: 20rpx;
  margin-bottom: 20rpx;
}
.image_frame_img {
  width: 180rpx;
  height: 180rpx;
  border-radius: 20rpx;
}
.image_frame_icon {
  position: absolute;
  width: 50rpx;
  height: 50rpx;
  top: -14rpx;
  right: -14rpx;
}
.upload_pic {
  width: 180rpx;
  height: 180rpx;
  border: 4rpx dashed #999;
  border-radius: 20rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  font-weight: 400;
  font-size: 28rpx;
  letter-spacing: -0.3px;
  color: #999;
  margin-bottom: 10rpx;
}
.camera_icon {
  font-size: 42rpx;
  margin-bottom: 16rpx;
}
.procedures_cards_complate {
  display: flex;
  justify-content: space-between;
}
.wx-switch-input{
  width: 80rpx !important;
  height: 50rpx !important;
}
.wx-switch-input::before{
  width: 80rpx !important;
  height: 46rpx !important;
  border: 1px solid #CDE7FF !important;
}
.wx-switch-input::after {
  width: 45rpx !important;
  height: 45rpx !important;
  background-color: #3F74F6 !important;
  border: 1px solid #CDE7FF !important;
}
.disableSwitch .wx-switch-input::before{
  border: 1px solid #E2E2E2 !important;
}
.disableSwitch .wx-switch-input::after {
  background-color: #E2E2E2 !important;
  border: 1px solid #E2E2E2 !important;
}

.save_btn {
  margin-top: 40rpx;
  width: 70%;
  height: 90rpx;
  line-height: 90rpx;
  font-weight: 700;
  font-size: 30rpx;
  letter-spacing: 1px;
  color: #fff;
  background: #3F74F6;
  border-radius: 46rpx;
}
.add_more {
  margin-top: 60rpx;
  width: 70%;
  height: 90rpx;
  line-height: 90rpx;
  font-weight: 700;
  font-size: 30rpx;
  letter-spacing: 1px;
  color: #fff;
  background: #3F74F6;
  border-radius: 46rpx;
}
/* 弹出层 */
.popup_addmore {
  background-color: #fff;
}
.popup_addmore_searchwrap {
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 30rpx 40rpx 0rpx;
}
.popup_addmore_searchinput {
  width: 65%;
  height: 60rpx;
  line-height: 60rpx;
  padding: 6rpx 20rpx;
  border: 1px solid #187FFF;
  border-radius: 20rpx;
  font-size: 28rpx;
}
.popup_addmore_searchbtn {
  margin: 0;
  padding: 0rpx 30rpx;
  font-weight: 700;
  font-size: 28rpx;
  letter-spacing: 4rpx;
  border-radius: 20rpx;
  color: #fff;
  background: #187FFF;
}

.popup_addmore_checkradio .t-radio {
  font-size: 28rpx;
  padding-top: 10rpx;
}
.popup_addmore_checkradio .t-radio__icon--left {
  margin-right: 0;
}
.popup_addmore_checkradio .t-radio__icon-dot {
  width: 60rpx;
  height: 60rpx;
}
.popup_addmore_checkradio .t-radio__icon-dot::after {
  width: 30rpx;
  height: 30rpx;
}
.popup_addmore_checkradio .t-radio__icon-circle::after {
  width: 60rpx;
  height: 60rpx;
}

.popup_addmore_title {
  padding: 20rpx 40rpx;
  font-size: 30rpx;
  font-weight: 600;
}
.popup_addmore_radiolist {
  padding: 0 40rpx;
  margin-bottom: 60rpx;
  height: 20vh;
  overflow-y: scroll;
}
.popup_addmore_radiolist .t-radio {
  padding: 10rpx;
  font-size: 28rpx;
}
.popup_addmore_radiolist .t-class-border {
  display: none;
}
.popup_addmore_radiolist .t-radio__icon--left {
  margin-right: 0;
}
.popup_addmore_radiolist .t-icon-base {
  font-size: 40rpx;
}
.popup_addmore_radiolist .t-radio__icon-dot::after {
  width: 30rpx;
  height: 30rpx;
}
.popup_addmore_radiolist .t-radio__icon-circle::after {
  width: 60rpx;
  height: 60rpx;
}

.popup_addmore_btnwrap {
  display: flex;
  align-items: center;
  justify-content: space-around;
}
.popup_addmore_btn {
  padding: 4rpx 60rpx;
  font-weight: 700;
  font-size: 28rpx;
  color: #187FFF;
  letter-spacing: 10rpx;
  border: 2rpx solid #187FFF;
  border-radius: 20rpx;
}
.confirmbtn {
  color: #fff;
  background: #187FFF;
}