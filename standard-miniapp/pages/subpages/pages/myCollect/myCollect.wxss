/* pages/subpages/pages/myCollect/myCollect.wxss */
.myCollect {
  position: relative;
  z-index: 2;
}
.store_title_class {
  background-color: #fff;
}
/* 顶部筛选容器 */
.myCollect_filterwrap {
  padding: 0 40rpx 40rpx;
  display: flex;
  align-items: center;
  height: 80rpx;
  justify-content: space-between;
}
.myCollect_filterwrap_icon {
  color: #333;
  font-size: 42rpx;
}
.myCollect_filterwrap_font {
  font-weight: 700;
  font-size: 28rpx;
  text-align: right;
  letter-spacing: 2px;
  color: #3F74F6;
}
.filterwrap_inputwrap {
  width: 80%;
  display: flex;
  padding: 16rpx 20rpx;
  align-items: center;
  background: #F5F6FA;
  border-radius: 10rpx;
  font-weight: 400;
  font-size: 26rpx;
  letter-spacing: -0.3px;
  color: #999;
}
.filterwrap_inputwrap_icon {
  font-size: 40rpx;
  margin-right: 10rpx;
}
/* 内容 */
.myCollect_contentwrap {
  padding: 40rpx 40rpx 80rpx;
  background-color: #f5f6fa;
}
.myCollect_contentwrap_cards {
  position: relative;
  padding: 40rpx 20rpx 40rpx 60rpx;
  background-color: #fff;
}
.myCollect_contentwrap_cards:first-child {
  border-radius: 20rpx 20rpx 0 0;
}
.myCollect_contentwrap_cards:last-child {
  border-radius: 0 0 20rpx 20rpx;
}
.myCollect_contentwrap_cards:not(:last-child) {
  border-bottom: 1px solid #E7E7E7;
}
.cards_title {
  font-weight: 600;
  font-size: 28rpx;
  letter-spacing: -0.3px;
  color: #333;
}
.cards_title_icon {
  position: absolute;
  top: 56rpx;
  transform: translate(-110%, -50%);
  font-size: 22rpx;
  color: #0F7CEE;
}
.cards_labels {
  margin: 20rpx 0;
  font-weight: 400;
  font-size: 24rpx;
}
.cards_labels_item {
  display: inline-block;
  padding: 6rpx 20rpx;
  margin: 6rpx;
  margin-right: 20rpx;
  text-align: center;
  color: #3F74F6;
  background-color: rgba(185, 205, 255, .6);
  opacity: 0.6;
  margin-bottom: 10rpx;
}
.orange_item {
  color: #FF5207;
  background-color: rgba(255, 205, 158, .6);
}
.policy_leave_css {
  color: #FF5207;
  background-color: #FFCD9E;
}
.policy_type_css {
  color: #BC7100;
  background-color: #FFE794;
}
.production_type_css {
  color: #FF1607;
  background-color: #FFBDAE;
}
.push_type_css {
  color: #2A9421;
  background-color: #D8FFD4;
}
.industry_info_css {
  color: #0878A9;
  background-color:  #BDEFFF;
}
.policy_object_css {
  color: #3C4C9F;
  background-color: #C9D2FF;
}
.policy_theme_css {
  color: #AE328D;
  background-color: #FFE1F7;
}
.cards_eles {
  color: #999;
  font-weight: 400;
  font-size: 26rpx;
  margin: 16rpx 0;
  display: flex;
  /* align-items: center; */
}
.cards_eles .eles_iconfont {
  margin-top: 2rpx;
  margin-right: 6rpx;
}
.cards_eles .cards_eles_label {
  word-break: keep-all;
}
.cards_eles .cards_eles_black {
  color: #333;
  font-weight: 600;
}
/* 弹出层 */
.filterpopup {
  margin-top: 40rpx;
  position: absolute;
  top: 80rpx;
  z-index: 3;
}
.filterpopup_new {
  margin-top: 40rpx;
  position: absolute;
  top: 80rpx;
  width: 100vw;
  height: 100vh;
  background-color: rgba(0, 0, 0, 0.8);
  z-index: 3;
}
.filterpopup_tagswrap {
  width: calc(100vw - 80rpx);
  padding: 40rpx;
  background-color: #fff;
  height: 15vh;
  overflow: hidden;
}
.filterpopup_tagswrap_content {
  height: 100%;
  overflow-y: scroll;
}
.tagswrap_card {
  display: inline-block;
  margin: 10rpx 10rpx;
}
/* 底部空元素 */
.empty {
  padding-bottom: calc(env(safe-area-inset-bottom) / 2);
}
.has_nodata {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  color: rgba(153, 153, 153, 0.60);
  font-family: PingFang SC;
  font-size: 28rpx;
  font-style: normal;
  font-weight: 400;
  line-height: normal;
  letter-spacing: 6rpx;
  padding: 40rpx 0;
}
.nodata_img {
  width: 300rpx;
  height: 300rpx;
}