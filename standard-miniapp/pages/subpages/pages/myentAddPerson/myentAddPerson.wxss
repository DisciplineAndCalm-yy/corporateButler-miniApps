/* pages/subpages/pages/myentAddPerson/myentAddPerson.wxss */
.myentAddPerson {
  position: relative;
  padding: 40rpx;
  background-color: #f5f6fa;
}
.store_title_class {
  background-color: #fff;
}
/* 企业信息 */
.myentAddPerson_entinfo {
  display: flex;
  padding: 40rpx;
  background-color: #fff;
  border-radius: 20px;
}
.myentAddPerson_entinfo_icons {
  width: 40rpx;
  height: 40rpx;
  line-height: 40rpx;
  font-size: 22rpx;
  text-align: center;
  color: #fff;
  background: #3F74F6;
  border-radius: 50%;
  margin-right: 20rpx;
}
.myentAddPerson_entinfo_entname {
  font-weight: 700;
  font-size: 28rpx;
  color: #333333;
  margin-bottom: 20rpx;
}
.myentAddPerson_entinfo_entcode {
  font-weight: 400;
  font-size: 26rpx;
  letter-spacing: -0.3px;
  color: #999999;
}
.myentAddPerson_entinfo_code {
  font-weight: 700;
  color: #333333;
}
/* 管家card */
.myentAddPerson_peoplewrap {
  margin-top: 20rpx;
  background-color: #fff;
  border-radius: 20px;
}
.myentAddPerson_peoplewrap_title {
  display: flex;
  align-items: center;
  height: 100rpx;
  line-height: 100rpx;
  font-weight: bold;
}
.peoplewrap_title_split {
  width: 8rpx;
  height: 46rpx;
  background: #0F7CEE;
  border-radius: 0px 24rpx 24rpx 0px;
  margin: 0 30rpx 0 -2rpx;
}
.peoplewrap_content {
  width: 100%;
}
.peoplewrap_content_cards {
  padding: 40rpx;
  display: flex;
  align-items: center;
  border-top: 1px solid #F2F2F2;
}
.peoplewrap_content_cards_icons {
  width: 70rpx;
  height: 70rpx;
  line-height: 70rpx;
  font-size: 28rpx;
  text-align: center;
  color: #fff;
  background: #3F74F6;
  border-radius: 50%;
  margin-right: 20rpx;
}
.peoplewrap_content_cards_infos {
  width: calc(100% - 100rpx);
  font-weight: 400;
  font-size: 28rpx;
}
.cards_infos_delete {
  text-align: right;
  color: #E33B3B;
}
.cards_infos_name {
  font-weight: 600;
  color: #333333;
}
.cards_infos_phone {
  margin-top: 10rpx;
  color: #999999;
}
.cards_infos_change {
  text-align: right;
  color: #0F7CEE;
}
.grey_font {
  color: #666;
}
/* 添加按钮 */
.myentAddPerson_addbtns {
  position: fixed;
  bottom: 20%;
  right: 0;
  padding: 20rpx 40rpx;
  color: #fff;
  font-weight: 400;
  font-size: 28rpx;
  background: rgba(63, 116, 246, 0.9);
  border-radius: 31.6147px 0px 0px 31.6147px;
  z-index: 99;
}
.myentAddPerson_addicons {
  margin-right: 10rpx;
}
/* 弹出层 */
.popupform {
  padding: 20rpx 40rpx;
}
.popupform_content_item {
  display: flex;
  padding: 20rpx 0;
  font-weight: 400;
  font-size: 28rpx;
  letter-spacing: -0.3px;
  align-items: center;
  justify-content: space-between;
  color: #999999;
}
.popupform_content_item:not(:first-child) {
  border-top: 1px solid #F2F2F2;
}
.popupform_content_item_label {
  width: 100rpx;
  margin-right: 40rpx;
}
.popupform_content_item_input {
  width: calc(100% - 150rpx);
  height: 100%;
  text-align: right;
}
.popupform_btnwrap {
  display: flex;
}
.popupform_btnwrap button {
  width: 200rpx;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 28rpx;
  font-weight: 400;
  color: #333;
  background-color: #fff;
}
.t-popup__content {
  width: 80vw;
}
.popupform_btnwrap .popupform_confirm {
  color: #fff;
  background-color: #0F7CEE;
}
/* 底部空元素 */
.empty {
  padding-bottom: calc(env(safe-area-inset-bottom) / 2);
}
.has_nodata {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  color: rgba(153, 153, 153, 0.60);
  font-family: PingFang SC;
  font-size: 28rpx;
  font-style: normal;
  font-weight: 400;
  line-height: normal;
  letter-spacing: 6rpx;
  padding: 40rpx 0;
}
.nodata_img {
  width: 300rpx;
  height: 300rpx;
}